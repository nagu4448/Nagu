using System;
using System.Linq;
using System.Collections.Generic;

namespace FirstProject;
// Implict constructor 
public class Implict
{
    int i; bool b;
    static void Main()
    {
        Implict p = new FirstProject.Implict();
        Console.WriteLine(p.i);
        Console.WriteLine(p.b);
        Console.ReadLine();
    }

}
